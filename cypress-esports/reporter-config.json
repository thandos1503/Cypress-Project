{
    "reporterEnabled": "mocha-junit-reporter, mochawesome, mocha-allure-reporter",
    "mochaJunitReporterReporterOptions": {
        "mochaFile": "cypress/reports/junit/test-results-[hash].xml"
    },
    "reporterOptions": {
        "reportDir": "cypress/reports/mochawesome",
        "charts": true,
        "overwrite": true,
        "html": false,
        "json":true
    }
}